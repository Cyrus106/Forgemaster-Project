<FTL><!--appending to Inferno-Core-->
	<events>
		<variable name="fm_called_forgemaster" op="set" val="0"/>
		<req name="FM_CAN_CALL_FORGEMASTER" type="all">
			<req>
				<name>fm_forgemaster_phone</name>
				<name>fm_forgemaster_dead</name>
			</req>
			<req lvl="0" max_lvl="0">
				<name>fm_called_forgemaster</name>
				<name>JUDGE_BOON_FM_FORGEWEAPON</name>
			</req>
		</req>
		<req name="FM_FORGEMASTER_UNCALLABLE">
			<name>fm_forgemaster_dead</name>
			<name>fm_forgemaster_called</name>
		</req>
		<req name="FM_FORGEMASTER_CALLABLE_NORMAL" type="all">
			<req max_lvl="0">
				<name>FM_FORGEMASTER_UNCALLABLE</name>
			</req>
			<req lvl="0" max_lvl="1">
				<name>challenge_level</name>
			</req>
		</req>
		<req name="FM_FORGEMASTER_CALLABLE_CHALLENGE" type="all">
			<req max_lvl="0">
				<name>FM_FORGEMASTER_UNCALLABLE</name>
			</req>
			<req lvl="2" max_lvl="3">
				<name>challenge_level</name>
			</req>
		</req>
	</events>
	<mod:findLike type="events">
		<mod:findName type="req" name="FMCORE_LIST_JUMPSPACE_EQUIPMENT">
			<mod-append:name>fm_forgemaster_phone</mod-append:name>
			<mod-append:name>fm_forgemaster_dead</mod-append:name>
		</mod:findName>
	</mod:findLike>
</FTL>

